#include <stdio.h>
#include <stdlib.h>
int main(){
	int num1, num2;
	
	printf("Digite o primeiro numero: ");
	scanf("%d",&num1);
	printf("Digite o segundo numero: ");
	scanf("%d",&num2);
	
	if((num1>0)&&(num2>0)){
		printf("Os 2 numeros sao positivos");
	}
	else if((num1<0)&&(num2<0)){
		printf("Os 2 numeros sao negativos");
	}
	
    if((num1<0)&&(num2>0)||(num1>0)&&(num2<0)){
	    printf("Um dos numeros e positivo e o outro negativo");
	
	}
	
}
